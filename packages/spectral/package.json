{
  "name": "@prismatic-io/spectral",
  "version": "9.0.0",
  "description": "Utility library for building Prismatic components",
  "keywords": [
    "prismatic"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://prismatic.io",
  "bin": {
    "component-manifest": "./dist/generators/componentManifest/cli.js"
  },
  "bugs": {
    "url": "https://github.com/prismatic-io/spectral"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prismatic-io/spectral.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn run format && yarn run lint && yarn run clean && tsc",
    "build:templates": "copyfiles --error -u 1 src/**/templates/**/* dist",
    "dev": "tsc -w",
    "cli:permission": "chmod +x dist/generators/**/cli.js",
    "postbuild": "yarn run build:templates && yarn run cli:permission",
    "prepack": "yarn run build",
    "format": "yarn run lint-fix && prettier --loglevel error --write 'src/**/*.ts' '*.{ts,js,json}' '!sidebars.{js,jse}'",
    "check-format": "prettier --check 'src/**/*.ts' '*.{ts,js,json}' '!sidebars.{js,jse}'",
    "check": "yarn run check-format && yarn run lint",
    "lint": "eslint --ext .ts .",
    "lint-fix": "eslint --fix --ext .ts .",
    "test": "jest",
    "tsd": "tsd",
    "docs": "rm -f sidebars.{js,jse} && typedoc"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@jsonforms/core": "3.0.0",
    "axios": "1.6.2",
    "axios-retry": "3.9.1",
    "date-fns": "2.30.0",
    "form-data": "4.0.0",
    "jest-mock": "29.7.0",
    "lodash": "4.17.21",
    "object-sizeof": "^2.6.4",
    "safe-stable-stringify": "2.3.1",
    "serialize-error": "8.1.0",
    "url-join": "5.0.0",
    "uuid": "8.3.2",
    "valid-url": "1.0.9",
    "yaml": "2.3.4"
  },
  "devDependencies": {
    "@types/ejs": "3.1.5",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.0",
    "@types/node": "14.14.35",
    "@types/sax": "1.2.4",
    "@types/url-join": "4.0.1",
    "@types/uuid": "8.3.4",
    "@types/valid-url": "1.0.3",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "copyfiles": "2.4.1",
    "ejs": "3.1.10",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-prettier": "4.0.0",
    "fast-check": "2.16.0",
    "jest": "29.7.0",
    "prettier": "2.6.2",
    "ts-jest": "29.1.2",
    "tsd": "0.31.0",
    "typedoc": "0.17.7",
    "typedoc-plugin-markdown": "2.4.2",
    "typedoc-plugin-remove-references": "0.0.5",
    "typescript": "4.6.3"
  },
  "tsd": {
    "directory": "./src/types-tests"
  }
}
