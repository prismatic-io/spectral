{
  "name": "@prismatic-io/spectral",
  "version": "7.0.7-pre",
  "description": "Utility library for building Prismatic components",
  "keywords": [
    "prismatic"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://prismatic.io",
  "bugs": {
    "url": "https://github.com/prismatic-io/spectral"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prismatic-io/spectral.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn run format && yarn run lint && yarn run clean && tsc",
    "prepack": "yarn run build",
    "format": "yarn run lint-fix && prettier --loglevel error --write 'src/**/*.ts' '*.{ts,js,json}' '!sidebars.{js,jse}'",
    "check-format": "prettier --check 'src/**/*.ts' '*.{ts,js,json}' '!sidebars.{js,jse}'",
    "check": "yarn run check-format && yarn run lint",
    "lint": "eslint --ext .ts .",
    "lint-fix": "eslint --fix --ext .ts .",
    "test": "jest",
    "tsd": "tsd",
    "docs": "rm -f sidebars.{js,jse} && typedoc"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "axios": "0.27.2",
    "axios-retry": "3.2.5",
    "date-fns": "2.28.0",
    "form-data": "4.0.0",
    "jest-mock": "27.0.3",
    "soap": "0.43.0",
    "uuid": "8.3.2",
    "valid-url": "1.0.9",
    "url-join": "5.0.0",
    "safe-stable-stringify": "2.3.1",
    "serialize-error": "8.1.0"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "14.14.35",
    "@types/sax": "1.2.4",
    "@types/uuid": "8.3.4",
    "@types/url-join": "4.0.1",
    "@types/valid-url": "1.0.3",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.1.3",
    "eslint-plugin-prettier": "4.0.0",
    "fast-check": "2.16.0",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "ts-jest": "27.0.3",
    "typedoc": "0.17.7",
    "typedoc-plugin-markdown": "2.4.2",
    "typedoc-plugin-remove-references": "0.0.5",
    "typescript": "4.6.3",
    "tsd": "0.20.0"
  },
  "tsd": {
    "directory": "./src/types-tests"
  }
}
