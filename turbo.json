{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "clean": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "generators/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "tsd": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "check": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "docs": {
      "outputs": ["docs/**"]
    },
    "all": {
      "dependsOn": ["check", "lint", "build", "test", "tsd"],
      "outputs": []
    }
  }
}
